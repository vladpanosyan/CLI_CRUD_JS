{
    "rules": {
        // "semi": ["error", "always"]
        // "quotes": ["error",]
        "no-var": 2,
		"no-const-assign": "error",
		"radix": "error",
		"prefer-template": "error",
		"prefer-const": "error",
		"prefer-spread": "error",
		"eqeqeq": ["error", "always"]
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 2017
    }
}